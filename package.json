{
	"name": "darkside",
	"displayName": "DarkSide",
	"description": "My VS Code extension",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.60.0"
	},
	"categories": [
		"Other"
	],
	"repository": {
		"type": "git",
		"url": ""
	},
	"activationEvents": [
		"onView:darkside-sidebar"
	],
	"main": "./dist/extension.js",
	"contributes": {
		"viewsContainers": {
			"activitybar": [
				{
					"id": "darkside-sidebar-view",
					"title": "Dark Side",
					"icon": "media/darth-vader.png"
				}
			]
		},
		"views": {
			"darkside-sidebar-view": [
				{
					"type": "webview",
					"id": "darkside-sidebar",
					"name": "DarkSide",
					"icon": "media/darth-vader.png",
					"contextualTitle": "DarkSide"
				}
			]
		},
		"commands": [
			{
				"command": "darkside.sayhello",
				"category": "DarkSide",
				"title": "Say hello"
			},
			{
				"command": "darkside.askquestion",
				"category": "DarkSide",
				"title": "Ask question"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run package",
		"compile": "webpack",
		"watch": "concurrently \"rollup -c -w\" \"webpack --mode development --watch\"",
		"package": "webpack --mode production --devtool hidden-source-map",
		"test-compile": "tsc -p ./",
		"test-watch": "tsc -watch -p ./",
		"pretest": "npm run test-compile && npm run lint",
		"lint": "eslint src --ext ts",
		"test": "node ./out/test/runTest.js"
	},
	"dependencies": {
		"vscode": "*"
	},
	"devDependencies": {
		"@rollup/plugin-commonjs": "^20.0.0",
		"@rollup/plugin-node-resolve": "^13.0.4",
		"@rollup/plugin-typescript": "^8.2.5",
		"@tsconfig/svelte": "^2.0.1",
		"@types/glob": "^7.1.3",
		"@types/mocha": "^8.2.2",
		"@types/node": "14.x",
		"@types/polka": "^0.5.3",
		"@types/vscode": "^1.60.0",
		"@typescript-eslint/eslint-plugin": "^4.26.0",
		"@typescript-eslint/parser": "^4.26.0",
		"concurrently": "^6.2.1",
		"eslint": "^7.27.0",
		"glob": "^7.1.7",
		"mocha": "^8.4.0",
		"rollup": "^2.56.3",
		"rollup-plugin-svelte": "^7.1.0",
		"rollup-plugin-terser": "^7.0.2",
		"svelte": "^3.42.6",
		"svelte-check": "^2.2.6",
		"svelte-preprocess": "^4.9.4",
		"ts-loader": "^9.2.2",
		"typescript": "^4.3.2",
		"vscode-test": "^1.5.2",
		"webpack": "^5.38.1",
		"webpack-cli": "^4.7.0"
	}
}